<aura:component controller="accListController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global" >
	<aura:attribute name="lstOfacc" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div class="slds-p-around_small">
    <aura:iteration items="{!v.lstOfacc}" var="acctObj">
       
        <p class="slds-p-horizontal_small">
        {!acctObj.Name} {!acctObj.Phone}<br/>
      </p>
       
    </aura:iteration>
        </div>
</aura:component>